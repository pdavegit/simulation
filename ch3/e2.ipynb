    "def e2(n):\n",
    "    if n <= 0:\n",
    "        return 3\n",
    "    else:\n",
    "        return (5*e2(n-1) + 7) % 200\n",
    "\n",
    "for n in range(1, 11):\n",
    "    print(n, e2(n))"
